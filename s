
function sayHiToGrandma(string){
  var cantHear = "I can't hear you!"
  if(string = "hello"){
    return cantHear
  }
}

function sayHiToGrandma(string){
  var indeed = "YES INDEED!"
  if(string = "HELLO"){
    return indeed
  }
}

function sayHiToGradma(string) {
var loveYouTou = "I love you too."
if(string = "I love you, Grandma") {
return loveYouToo
}
}
